<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.web.WebView?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.111"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.ephr.controllers.LoginController">
    <children>
        <VBox spacing="10" alignment="CENTER">
            <Label text="Login to EPHR" style="-fx-font-size: 18px; -fx-font-weight: bold;" />
            <WebView fx:id="webView" prefHeight="600" prefWidth="400"/>
            <Label fx:id="errorLabel" text="" style="-fx-text-fill: red;" />
        </VBox>
    </children>
</AnchorPane>
